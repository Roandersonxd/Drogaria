<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.roanderson.projeto" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.MyTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".SecondActivity"
            android:label="@string/title_activity_second" />
        <activity
            android:name=".principal"
            android:label="@string/title_activity_principal" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainFabricante"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".MainListaFabricante"
            android:label="@string/title_activity_main_lista_fabricante" >
        </activity>
        <activity
            android:name=".MainVenda"
            android:label="@string/title_activity_main_venda" >
        </activity>
        <activity
            android:name=".MainProduto"
            android:label="@string/title_activity_main_produto" >
        </activity>
        <activity
            android:name=".MainListaProdutos"
            android:label="@string/title_activity_main_lista_produtos" >
        </activity>
    </application>

</manifest>
